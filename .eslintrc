{
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": [
		"eslint:recommended",
		"eslint-config-prettier",
		"plugin:jest/recommended"
	],
	"rules": {
		"strict": ["error", "never"]
	},
	"env": {
		"browser": true
	},
	"plugins": ["jest"]
}
